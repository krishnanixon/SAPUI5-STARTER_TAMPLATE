<mvc:View
	controllerName="nixgo.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button
				icon="sap-icon://hello-world"
				press=".onOpenDialog"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="nixgo.view.HelloPanel"/>
			<mvc:XMLView viewName="nixgo.view.InvoiceList"/>
		</content>
	</Page>
</mvc:View>
